{
  "name": "arraypress/wp-stasher",
  "version": "1.0.0",
  "description": "A utility for capturing and storing WordPress filter values for later use within the same request.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-stasher/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-stasher/issues",
    "source": "https://github.com/arraypress/wp-stasher"
  },
  "keywords": [
    "wordpress",
    "filters",
    "hooks",
    "utilities",
    "filter-values",
    "data-storage",
    "temporary-storage",
    "value-capture",
    "filter-capture",
    "hook-values"
  ]
}